{
  "name": "aria-dashboard-spa",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:js\"",
    "dev:css": "npx @tailwindcss/cli -i src/index.css -o dist/bundle.css --watch",
    "dev:js": "node esbuild.config.mjs --watch",
    "build": "npm run build:css && npm run build:js && node -e \"const fs=require('fs');const v=Date.now();fs.writeFileSync('dist/index.html',fs.readFileSync('index.html','utf8').replace(/bundle\\.css/g,'bundle.css?v='+v).replace(/bundle\\.js/g,'bundle.js?v='+v))\"",
    "build:css": "npx @tailwindcss/cli -i src/index.css -o dist/bundle.css --minify",
    "build:js": "node esbuild.config.mjs"
  },
  "dependencies": {
    "@preact/signals": "^2.8.1",
    "preact": "^10.25.0",
    "preact-router": "^4.1.2",
    "superhot-ui": "file:../../../../superhot-ui",
    "uplot": "^1.6.32"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "concurrently": "^9.1.0",
    "esbuild": "^0.27.3",
    "tailwindcss": "^4.0.0"
  }
}
